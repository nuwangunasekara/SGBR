    ["Oza(SGBT(FIRTDD))", "Z",  "meta.OzaBag -s 100 -l (meta.StreamingGradientBoostedTrees -t -L 1.0 -m 75 -s 1 -l (trees.FIMTDD -s VarianceReductionSplitCriterion -g 50 -c 0.01 -e))"]
