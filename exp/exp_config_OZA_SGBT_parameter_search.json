    ["Oza(SGBT(FIRTDD))", "Z",  "meta.OzaBag -s 5 -l (meta.StreamingGradientBoostedTrees -t -L 1.0 -m 75 -s 20 -l (trees.FIMTDD -s VarianceReductionSplitCriterion -g 50 -c 0.01 -e))"],
    ["Oza(SGBT(FIRTDD))", "Z",  "meta.OzaBag -s 10 -l (meta.StreamingGradientBoostedTrees -t -L 1.0 -m 75 -s 10 -l (trees.FIMTDD -s VarianceReductionSplitCriterion -g 50 -c 0.01 -e))"],
    ["Oza(SGBT(FIRTDD))", "Z",  "meta.OzaBag -s 20 -l (meta.StreamingGradientBoostedTrees -t -L 1.0 -m 75 -s 5 -l (trees.FIMTDD -s VarianceReductionSplitCriterion -g 50 -c 0.01 -e))"],
    ["Oza(SGBT(FIRTDD))", "Z",  "meta.OzaBag -s 25 -l (meta.StreamingGradientBoostedTrees -t -L 1.0 -m 75 -s 4 -l (trees.FIMTDD -s VarianceReductionSplitCriterion -g 50 -c 0.01 -e))"],
